{
    "name": "TGCN",
   
    "saved_dir": "tgcn_result/vis/TGCN",
    "loss_metrics":{
        "input_path": "tgcn_result/train/loss_metrics.json",
        "saved_sub_dir":"loss_metrics",
        "loss" : {
            "dynamic" : false, 
            "figsize" : [8,4],
            "color": "black",
            "linewidth": 2,
            "dpi": 100

        },
        "metrics":{
            "strategy": "all", // single or all

            "color" : "red", 
            "linewidth": 2,
            "dpi": 100,
            
